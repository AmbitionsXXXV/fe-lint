    "pnpm": "^8.8.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.24.8",
    "typescript": "^5.2.2",
    "vue-template-compiler": "^2.7.14",
    "vuepress": "^1.9.10",
    "vuepress-plugin-one-click-copy": "^1.0.6",
    "vuepress-plugin-zooming": "^1.1.8"
  },
  "dependencies": {
